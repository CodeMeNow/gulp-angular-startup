{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "rules": {
    "angular/no-service-method": 0,
    "angular/log": 0,
    "no-console": 0,
    "indent": [2, 2, {"SwitchCase": 1, "VariableDeclarator": 1}],
    "valid-jsdoc": 0,
    "complexity": [1, 20],
    "curly": 2,
    "eqeqeq": [2, "smart"],
    "no-multi-spaces": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-use-before-define": 0,
    "no-unused-vars": [2],
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": 2,
    "comma-spacing": 2,
    "computed-property-spacing": 2,
    "func-names": 2,
    "key-spacing": 2,
    "max-depth": [1, 3],
    "max-len": [2, 120, 4],
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2, {max: 2}],
    "object-curly-spacing": 2,
    "operator-linebreak": 2,
    "semi-spacing": 2,
    "semi": 2,
    "space-after-keywords": [2, "always"],
    "space-before-keywords": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "quotes": [2, "single"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "moment": true,
    "testSuite": true,
    "$": true
  }
}
